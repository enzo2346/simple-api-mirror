{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "default": {
      "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
    },
    "error": {
      "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "formatter": "default",
      "stream": "ext://sys.stdout"
    },
    "error_console": {
      "class": "logging.StreamHandler",
      "formatter": "error",
      "stream": "ext://sys.stderr"
    }
  },
  "loggers": {
    "uvicorn": {
      "level": "INFO",
      "handlers": ["console"],
      "propagate": false
    },
    "uvicorn.error": {
      "level": "ERROR",
      "handlers": ["error_console"],
      "propagate": false
    }
  },
  "root": {
    "level": "DEBUG",
    "handlers": ["console"],
    "propagate": false
  }
}
